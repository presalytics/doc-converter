{
    "paths": {
        "/svgconvert": {
            "post": {
                "summary": "converts pptx file to svg image",
                "requestBody": {
                    "description": "Filepath to pptx file",
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "filename": [
                                            {
                                                "type": "string",
                                                "format": "binary"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Url of svg file",
                        "content": {
                            "application/json": {
                                "schema": [
                                    {
                                        "type": "string",
                                        "description": "url of converted file"
                                    }
                                ]
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request."
                    },
                    "415": {
                        "description": "Invalid file type"
                    }
                }
            }
        },
        "/": {
            "get": {
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/[{'description': 'OK', 'content': {'application/json': {'schema': [{'type': 'string', 'description': 'hello world'}]}}}]"
                    }
                }
            }
        }
    },
    "info": {
        "title": "Doc Converter API",
        "version": "0.1"
    },
    "openapi": "3.0.2"
}